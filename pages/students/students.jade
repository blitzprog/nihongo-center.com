if !user
	p Not logged in!
else if typeof students === "undefined"
	p You are not authorized to view this page!
else if students.length === 0
	p No students registered yet!
else
	.page-header
		h2 Students
	
	table.table.table-condensed
		thead
			tr
				th Applied
				th Completion
				th Course
				th Country
				th Name
		each student in students
			tr
				td.col-md-1
					if student.applicationDate === null
						span ❌
					else
						span ✓
				td.col-md-1
					meter(min=0, max=100, low=25, value=student.profileCompleted, title=student.profileCompleted + "%")
				td.col-md-2
					.form-group
						input.form-control(type="month", value=student.startYear + "-" + student.startMonth, disabled)
				td.col-md-2= student.country
				td.col-md-6
					a(href=student.permaLink)= student.givenName + " " + student.familyName