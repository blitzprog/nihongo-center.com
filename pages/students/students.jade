if typeof students === "undefined"
	p You are not authorized to view this page!
else if students.length === 0
	p No students registered yet!
else
	.page-header
		h2 Students
	
	table.table
		thead
			tr
				th Course
				th Country
				th Name
		each student in students
			tr
				td.col-md-2
					.form-group
						input.form-control(type="month", value=student.startYear + "-" + student.startMonth, disabled)
				td.col-md-2= student.country
				td.col-md-8
					a(href=student.permaLink)= student.givenName + " " + student.familyName